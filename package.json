{
  "name": "quickbom",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start -p ${PORT:-3000}",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "node data/seeds/index.js",
    "db:clear": "./clear-database.sh",
    "db:clear:supabase": "node clear-supabase.js",
    "db:clear:supabase:confirm": "CONFIRM_DELETE=yes NODE_ENV=production node clear-supabase.js",
    "deploy:check": "NODE_ENV=production node -e \"require('./lib/prisma').checkDatabaseConnection().then(r => console.log(r.connected ? '✅ DB Connected' : '❌ DB Failed')).catch(console.error)\"",
    "backup:create": "node -e \"require('./lib/backup-service.ts').backupService.createBackup().then(r => console.log('Backup created:', r)).catch(console.error)\"",
    "backup:cleanup": "node -e \"require('./lib/backup-service.ts').backupService.cleanupOldBackups().then(r => console.log('Cleaned up', r, 'old backups')).catch(console.error)\"",
    "backup:start": "node -e \"require('./lib/backup-scheduler.ts').backupScheduler.start(); console.log('Backup scheduler started')\"",
    "backup:stop": "node -e \"require('./lib/backup-scheduler.ts').backupScheduler.stop(); console.log('Backup scheduler stopped')\"",
    "backup:test": "node test-backup-system.js",
    "vercel:build": "NODE_ENV=production npm run build"
  },
